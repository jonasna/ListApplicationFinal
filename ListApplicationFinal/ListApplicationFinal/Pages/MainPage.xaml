<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:ListApplicationFinal"
             xmlns:forms="clr-namespace:FFImageLoading.Svg.Forms;assembly=FFImageLoading.Svg.Forms"
             xmlns:markup="clr-namespace:ListApplicationFinal.Markup"
             xmlns:customcontrols="clr-namespace:ListApplicationFinal.CustomControls"
             xmlns:buttons="clr-namespace:ListApplicationFinal.CustomControls.Buttons"
             x:Class="ListApplicationFinal.Pages.MainPage"
             Title="{Binding Title}">

    <StackLayout>
        <!-- Place new controls here -->
    	<Label Text="Welcome to Xamarin.Forms!"/>

        <buttons:ImageButton Source="{markup:ImageResource delete.svg}" WidthRequest="70" HeightRequest="70" CornerRadius="35"></buttons:ImageButton>
        <buttons:ImageButton Source="{markup:ImageResource button.svg}" WidthRequest="150" HeightRequest="150" CornerRadius="75"></buttons:ImageButton>

    </StackLayout>

</ContentPage>
